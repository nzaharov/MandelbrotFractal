    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rsa_course_project'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rsa_course_project",
                    "--package=rsa_course_project"
                ],
                "filter": {
                    "name": "rsa_course_project",
                    "kind": "bin"
                }
            },
            "args": [
                "-s",
                "1000x1000",
                "-r",
                "0.25:0.35:-0.05:0.05"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'rsa_course_project'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=rsa_course_project",
                    "--package=rsa_course_project"
                ],
                "filter": {
                    "name": "rsa_course_project",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}